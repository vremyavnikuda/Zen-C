
// Test if primitive type methods work via naming convention

impl int {
    fn abs(self) -> int {
        if (*self < 0) return -(*self);
        return *self;
    }
    
    fn is_positive(self) -> bool {
        return *self > 0;
    }
}

fn main() {
    let x: int = -42;
    let abs_x = x.abs();
    println "abs(-42) = {abs_x}";
    
    let is_pos = x.is_positive();
    println "-42 is positive: {is_pos}";
    
    // Test literal method call
    let lit_abs = (-123).abs();
    println "abs(-123) = {lit_abs}";

    return 0;
}
