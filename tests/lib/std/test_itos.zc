
import "std.zc"

test "itos_basic" {
    let s = itos(42);
    assert(strcmp(s, "42") == 0, "itos(42) should return \"42\"");
    
    let s2 = itos(-123);
    assert(strcmp(s2, "-123") == 0, "itos(-123) should return \"-123\"");
    
    let s3 = utos(999);
    assert(strcmp(s3, "999") == 0, "utos(999) should return \"999\"");
    
    println "  itos tests: Success";
}
